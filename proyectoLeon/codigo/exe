    %jmp T_6.5;
T_6.4 ;
    %mov 40, 2, 24;
T_6.5 ;
    %mov 8, 40, 24; Move signal select into place
    %load/v 32, v0x2326970_0, 8; Select 8 out of 32 bits
    %set/v v0x2327420_0, 8, 32;
    %load/v 8, v0x2327420_0, 32;
    %load/v 40, v0x2327260_0, 8;
    %mov 48, 0, 24;
