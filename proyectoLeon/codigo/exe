TD_tester.test.drv_init ;
    %set/v v0x2327ef0_0, 0, 32;
T_0.0 ;
    %load/v 8, v0x2327ef0_0, 32;
   %cmpi/s 8, 4, 32;
    %jmp/0xz T_0.1, 5;
    %ix/getv/s 3, v0x2327ef0_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x23272e0, 0, 32;
t_0 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x2327ef0_0, 32;
    %set/v v0x2327ef0_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %set/v v0x2327ef0_0, 0, 32;
T_0.2 ;
    %load/v 8, v0x2327ef0_0, 32;
   %cmpi/s 8, 16, 32;
    %jmp/0xz T_0.3, 5;
    %movi 8, 5, 33;
    %load/v 41, v0x2327ef0_0, 32;
    %mov 73, 72, 1;
    %add 8, 41, 33;
    %ix/getv/s 3, v0x2327ef0_0;
   %jmp/1 t_1, 4;
   %ix/load 1, 0, 0;
   %set/av v0x2327ff0, 8, 8;
t_1 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x2327ef0_0, 32;
    %set/v v0x2327ef0_0, 8, 32;
    %jmp T_0.2;
T_0.3 ;
    %wait E_0x2327770;
    %set/v v0x23281d0_0, 1, 1;
    %wait E_0x2327770;
    %set/v v0x23281d0_0, 0, 1;
    %end;
